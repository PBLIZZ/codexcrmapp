{
  "name": "codexcrmapp",
  "version": "0.1.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cd apps/web && next dev",
    "build": "pnpm --filter @codexcrm/web run build",
    "start": "cd apps/web && next start",
    "prepare": "husky",
    "lint": "eslint .",
    "typecheck": "tsc -p apps/web/tsconfig.json --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,scss,less,html,js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@shadcn/ui": "^0.0.4",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/papaparse": "^5.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.501.0",
    "next": "15.3.1",
    "papaparse": "^5.5.3",
    "react": "^19.0.2",
    "react-dom": "^19.0.2",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.56.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tw-animate-css": "^1.2.5",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    },
    "onlyBuiltDependencies": [
      "@sentry/cli",
      "@swc/core",
      "@tailwindcss/oxide",
      "esbuild",
      "supabase"
    ]
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.3.2",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@types/google-one-tap": "^1.2.6",
    "@types/google.accounts": "^0.0.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "supabase": "^2.24.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.32.1"
  }
}
